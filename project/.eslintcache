   </Form.Item>\r\n\r\n        <Form.Item label=\"תאריך לידה\" >\r\n          <DatePicker required placeholder=\"בחר תאריך\" onChange={this.chgBirthDate}\r\n            onFocus={() => { this.setState({ errors: {} }) }} />\r\n          <div className=\"text-danger\">{this.state.errors.birthDate}</div>\r\n        </Form.Item>\r\n\r\n        <Form.Item label=\"עיר קבע\" name=\"city\" required>\r\n          <Select placeholder=\"בחר עיר\"\r\n            onChange={this.chgCity}\r\n            onFocus={() => { this.setState({ errors: {} }) }}\r\n          >\r\n            <Select.Option value=\"choose\">בחר עיר</Select.Option>\r\n            {citiesList.map((city) => (\r\n              <Select.Option key={city} value={city}>{city}</Select.Option>\r\n            ))}\r\n          </Select>\r\n          <div className=\"text-danger\">{this.state.errors.city}</div>\r\n        </Form.Item>\r\n\r\n        <Form.Item required label=\"מקום מגורים נוכחי\">\r\n          <Select placeholder=\"בחר עיר\"\r\n            onChange={this.chgCurrentCity}\r\n            onFocus={() => { this.setState({ errors: {} }) }}\r\n          >\r\n            <Select.Option value=\"choose\">בחר עיר</Select.Option>\r\n            {citiesList.map((city) => (\r\n              <Select.Option key={city} value={city}> {city} </Select.Option>\r\n            ))}\r\n          </Select>\r\n          <div className=\"text-danger\">{this.state.errors.currentCity}</div>\r\n        </Form.Item>\r\n\r\n        <Form.Item label=\"סטטוס\">\r\n          <Select placeholder=\"בחר\" onChange={this.chgStatus}>\r\n            <Select.Option value=\"choose\">בחר</Select.Option>\r\n            <Select.Option value=\"single\">רווק/ה</Select.Option>\r\n            <Select.Option value=\"married\">נשוי/ה</Select.Option>\r\n            <Select.Option value=\"inPublic\">ידוע/ה בציבור</Select.Option>\r\n            <Select.Option value=\"widow\">אלמן/ה</Select.Option>\r\n          </Select>\r\n        </Form.Item>\r\n        <Form.Item>\r\n          <Button\r\n            type='primary'\r\n            shape='round'\r\n            onClick={this.btnNext}> הבא </Button>\r\n        </Form.Item>\r\n      </Form>\r\n      </div>\r\n\r\n    )\r\n  }\r\n}\r\n\r\nexport default withRouter(CCSignin3);\r\n","C:\\Users\\User\\Documents\\GitHub\\ReactApp\\ProjectReact\\project\\src\\CitiesFile.jsx",["79"],"import React from 'react';\r\n\r\nexport  const CitiesFile = [\r\n    'משמר העמק',\r\n    'נתניה',\r\n    'כפר יונה',\r\n    'עין המפרץ',\r\n    'מעגן מיכאל',\r\n    'חיפה',\r\n    'תל אביב',\r\n] \r\n\r\nexport default CitiesFile;",{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":3,"column":8,"nodeType":"86","messageId":"87","endLine":3,"endColumn":18},{"ruleId":"84","severity":1,"message":"88","line":15,"column":7,"nodeType":"86","messageId":"87","endLine":15,"endColumn":15},{"ruleId":"89","severity":1,"message":"90","line":16,"column":6,"nodeType":"91","endLine":17,"endColumn":17},{"ruleId":"84","severity":1,"message":"92","line":20,"column":12,"nodeType":"86","messageId":"87","endLine":20,"endColumn":28},{"ruleId":"84","severity":1,"message":"93","line":6,"column":8,"nodeType":"86","messageId":"87","endLine":6,"endColumn":18},{"ruleId":"84","severity":1,"message":"94","line":7,"column":8,"nodeType":"86","messageId":"87","endLine":7,"endColumn":17},{"ruleId":"84","severity":1,"message":"95","line":12,"column":8,"nodeType":"86","messageId":"87","endLine":12,"endColumn":18},{"ruleId":"84","severity":1,"message":"96","line":7,"column":8,"nodeType":"86","messageId":"87","endLine":7,"endColumn":18},{"ruleId":"84","severity":1,"message":"85","line":8,"column":8,"nodeType":"86","messageId":"87","endLine":8,"endColumn":18},{"ruleId":"84","severity":1,"message":"97","line":4,"column":10,"nodeType":"86","messageId":"87","endLine":4,"endColumn":14},{"ruleId":"84","severity":1,"message":"98","line":5,"column":8,"nodeType":"86","messageId":"87"